#include <iostream>
using namespace std;
int main(){
	int n;
	cin >> n;
	long int a,b,c;	
	for(int i=1; i<=n;i++){			
		cin >> a >> b >> c;
		if(a+b > c){
			cout << "Case #" << i << ": true\n";  // Case #X: true
		}else{
			cout << "Case #" << i << ": false\n";  // Case #X: false
		}		
	}
	
	return 0;	
} 


//====另一种写法=============
#include <iostream>
using namespace std;
int main(){
	int n;
	scanf("%d",&n);			
	for(int i = 1 ;i<=n;i++){
		long long int a,b,c;
		scanf("%lld%lld%lld",&a,&b,&c);
		printf("Case #%d: %s\n",i,a+b>c?"true":"false");
	}
	return 0;	
}
