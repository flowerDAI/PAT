#include <iostream>
#include <vector>
using namespace std;
int main(){
	vector<int> v[5];
	
	int num;
	
	for (int i = 0; i < 5; i++) {
		
		for (int j=0; j< 3;j++){
			cin >> num;
			v[i].push_back(num);
		}        
    }
    
    for(int i=0; i< 5 ; i++) //输出二维动态数组 
    {
        for(int j=0;j<v[i].size();j++)
        {
            cout<<v[i][j]<<" ";
        }
        cout<<"\n";
    }
    
    cout << "======================================="<<endl;
    
    for(int j=0;j<v[3].size();j++)
    {
        cout<<v[3][j]<<" ";
    }
    cout<<"\n";
    
    
    return 0;
} 
